#!/bin/bash

./pack
# gulp bump --patch
cordova build ios --device --release
xcrun \
  -sdk iphoneos PackageApplication \
  -v "platforms/ios/build/device/HelloCordova.app" \
  -o "/Users/eteng/hellocordova.ipa" \
  --sign "iPhone Distribution: CNAMPGC Holding Co., Ltd. (2H2M5TDW9J)" \
  --embed "nhw360_distribute.mobileprovision"
