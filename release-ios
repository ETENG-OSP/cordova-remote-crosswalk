#!/bin/bash

./pack
# gulp bump --patch
cordova build ios --device --release
xcrun \
  -sdk iphoneos PackageApplication \
  -v "platforms/ios/build/device/农惠网.app" \
  -o "/Users/eteng/nhw360.ipa" \
  --sign "iPhone Distribution: CNAMPGC Holding Co., Ltd. (2H2M5TDW9J)" \
  --embed "signatures/nhw360_distribute.mobileprovision"
